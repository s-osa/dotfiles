[user]
  name = OSA Shunsuke
  email = hhelibebcnofnenamg@gmail.com
[core]
  editor = vim
  autocrlf = input
  excludesfile = ~/.gitignore
[color]
  ui = auto
[push]
  default = simple
[credential]
  helper = osxkeychain
[ghq]
  root = ~/src
[alias]
  s = status
  co = checkout
  a = add .
  ap = add -p
  d = diff
  dc = diff --cached
  amend = commit --amend
  l = log
  lp = log -p
  b = branch

[includeIf "gitdir:~/src/github.com/agrigate-team/"]
	path = ~/.gitconfig.agrigate
